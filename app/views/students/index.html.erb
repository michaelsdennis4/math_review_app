<a href="/dashboard">Dashboard</a>

<% if (session[:is_teacher]) %>

	<h1>STUDENT INDEX</h1>

	<ul>

		<% @students.each do |student| %>

			<li><a href="/students/<%= student.id %>"><%= student.first_name %> <%= student.last_name %></a></li>

		<% end %>

	</ul>

<% else %>

	<h1>Sorry, you are not authorized to view this page.</h1>

<% end %>


